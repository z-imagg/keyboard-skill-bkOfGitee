#file path: ~/.config/sxhkd/sxhkdrc

#重启sxhkd
alt + z + z ;
       /app/sxhkd_reboot_delay_async.sh

#激活webstorm窗口
alt + w + s ;
      /app/xdotool_script/win_activ.sh  webstorm

#激活firefox窗口
alt + f + f ;
      /app/xdotool_script/win_activ.sh firefox

#激活gedit窗口或新启动gedit
alt + g + e ; 
      /app/xdotool_script/win_activ_or_boot_new.sh gedit
 
#激活gnome-terminal窗口或新启动gnome-terminal
alt + g + t ;
      /app/xdotool_script/win_activ_or_boot_new.sh gnome-terminal 
 